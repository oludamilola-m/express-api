{
  "name": "express-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed:generate": "npx sequelize-cli seed:generate",
    "db:reset": "npx sequelize-cli db:drop && npm run db:create && npm run db:migrate && npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^8.3.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0"
  }
}
